#!/bin/bash

NODE=`which node`
NODEJS=`which nodejs`
IOJS=`which iojs`

if [[ -n $NODE ]]; then
  $NODE $($NODE -e "console.log(require.resolve('pre-commit'))")
elif [[ -n $NODEJS ]]; then
  $NODEJS $($NODEJS -e "console.log(require.resolve('pre-commit'))")
elif [[ -n $IOJS ]]; then
  $IOJS $($IOJS -e "console.log(require.resolve('pre-commit'))")
fi
